 src/qt/bitcoin.qrc | 7 +++----
 1 file changed, 3 insertions(+), 4 deletions(-)

diff --git a/src/qt/bitcoin.qrc b/src/qt/bitcoin.qrc
index ff579e7..fadc940 100644
--- a/src/qt/bitcoin.qrc
+++ b/src/qt/bitcoin.qrc
@@ -1,10 +1,10 @@
 <RCC>
     <qresource prefix="/icons">
